   "source": [
    "nums = [1, 35, 12, 24, 31, 51, 70, 100]\n",
    "\n",
    "def count(numbers):\n",
    "    total = 0\n",
    "    i = 0  # index\n",
    "\n",
    "    while i < len(numbers):\n",
    "        if numbers[i] < 20:\n",
    "            total += 1\n",
    "        i += 1  # move to next index\n",
    "\n",
    "    return total\n",
    "\n",
    "count(nums)\n",
    "\n",
    "# 2\n"
   ]
